class Solution {
  public:
    vector<int> getBuildingsWithAView(vector<int>& buildings) {
      vector<int> ans;
      int max = -1;
      for(int i = 0;i<buildings.size();i++)
      {
        if(buildings[i]>max)
        {
          max = buildings[i];
          ans.push_back(i);
        }
      }
      
    return ans;
    }
};
